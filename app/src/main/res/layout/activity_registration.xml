<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Registration">

    <android.support.constraint.Barrier android:layout_width="wrap_content" android:layout_height="wrap_content"
                                        android:id="@+id/barrier" app:barrierDirection="top"
                                        tools:layout_editor_absoluteY="823dp"/>
    <EditText
            android:layout_width="227dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="FirstName"
            android:ems="10"
            android:id="@+id/etFirstName"
            android:hint="FirstName" tools:layout_editor_absoluteY="37dp" tools:layout_editor_absoluteX="122dp"/>
    <EditText
            android:layout_width="286dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="LastName"
            android:ems="10"
            android:id="@+id/etLastName"
            android:hint="LastName"
            tools:layout_editor_absoluteY="141dp" tools:layout_editor_absoluteX="117dp"/>
    <EditText
            android:layout_width="286dp"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/etEmail"
            android:hint="Email"
            android:text="Email" tools:layout_editor_absoluteY="232dp" tools:layout_editor_absoluteX="117dp"/>
    <EditText
            android:layout_width="287dp"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/etPhonenumber" android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/etEmail" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" android:layout_marginRight="16dp" android:hint="Phonenumber"
            android:text="Phonenumber" app:layout_constraintHorizontal_bias="1.0"/>
    <EditText
            android:layout_width="288dp"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/etpassword" android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/etPhonenumber" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" android:layout_marginRight="16dp" android:hint="Password"
            app:layout_constraintHorizontal_bias="1.0"/>
    <EditText
            android:layout_width="287dp"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            tools:layout_editor_absoluteY="498dp" tools:layout_editor_absoluteX="116dp"
            android:id="@+id/etComfirmpassword" android:hint="Comfirm password"/>
    <Button
            android:text="Sign up"
            android:layout_width="144dp"
            android:layout_height="80dp"
            android:id="@+id/btnSignup" android:layout_marginTop="96dp"
            app:layout_constraintTop_toBottomOf="@+id/etComfirmpassword" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" android:layout_marginRight="16dp"
            app:layout_constraintHorizontal_bias="0.497"/>
    <ImageView
            android:layout_width="103dp"
            android:layout_height="97dp" tools:srcCompat="@tools:sample/avatars"
            tools:layout_editor_absoluteY="0dp" tools:layout_editor_absoluteX="6dp"/>
    <ImageView
            android:layout_width="103dp"
            android:layout_height="77dp" tools:srcCompat="@tools:sample/avatars[10]"
            tools:layout_editor_absoluteY="109dp" tools:layout_editor_absoluteX="6dp" android:id="@+id/imageView3"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_dialog_email"
            tools:layout_editor_absoluteY="57dp" tools:layout_editor_absoluteX="0dp" android:id="@+id/imageView4"/>
    <ImageView
            android:layout_width="109dp"
            android:layout_height="91dp" app:srcCompat="@android:drawable/ic_dialog_email"
            android:id="@+id/imageView5"
            app:layout_constraintEnd_toStartOf="@+id/etEmail"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"/>
    <ImageView
            android:layout_width="112dp"
            android:layout_height="85dp" app:srcCompat="@android:drawable/ic_menu_call"
            tools:layout_editor_absoluteY="276dp" tools:layout_editor_absoluteX="-5dp" android:id="@+id/imageView6"/>
    <ImageView
            android:layout_width="107dp"
            android:layout_height="97dp" app:srcCompat="?attr/actionModePasteDrawable"
            android:id="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/imageView6" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/etpassword"
            app:layout_constraintHorizontal_bias="0.0"/>
    <ImageView
            android:layout_width="117dp"
            android:layout_height="82dp" app:srcCompat="@android:drawable/ic_menu_my_calendar"
            tools:layout_editor_absoluteY="480dp" tools:layout_editor_absoluteX="-10dp" android:id="@+id/imageView8"/>
</android.support.constraint.ConstraintLayout>